//方法一：HashMap
HashMap<Character,Integer> hashMap=new HashMap<>();
        for(int i=0;i<J.length();i++){
            char ch=J.charAt(i);
            hashMap.put(ch,0);
        }
        int numbers=0;
        for(int j=0;j<S.length();j++){
            char ch=S.charAt(j);
            if(hashMap.get(ch)!=null){
                numbers++;
            }
        }
        return numbers;
