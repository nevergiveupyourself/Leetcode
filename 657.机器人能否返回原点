class Solution {
    public boolean judgeCircle(String moves) {
        int x=0,y=0;
            for(char move:moves.toCharArray()){
                switch (move){
                    case 'R':
                        x=x+1;
                        break;
                    case 'L':
                        x=x-1;
                        break;
                    case 'U':
                        y=y+1;
                        break;
                    case 'D':
                        y=y-1;
                        break;
                    default:
                        break;
                }
            }
            if(x==0 && y==0)
                return true;
            return false;
    }
}
